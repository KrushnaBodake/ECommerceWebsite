@model IEnumerable<ECommerceWebsite.Models.Order>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>   
    <a href="~/UserProduct/Index" class="btn btn-primary btn-sm">Order New Product</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Product Id
            </th>
            <th>
                User Id
            </th>
            <th>
                Quantity
            </th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @item.ProdId
            </td>
            <td>
                @item.Id
            </td>
            <td>
                @item.Qty
            </td>
            <td>
                    @Html.ActionLink("Edit", "Edit", new { id=item.Oid }) |
                    @Html.ActionLink("Details", "Details", new {  id=item.Oid }) |
                    @Html.ActionLink("Delete", "Delete", new {  id=item.Oid })
            </td>
        </tr>
}
    </tbody>
</table>
